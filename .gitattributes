package actividad08;

public class Arreglo {
	
	
	
	
	public static int existeArre(int[] arreglo, int busqueda) {
		  for (int x = 0; x < arreglo.length; x++) {
		    if (arreglo[x] == busqueda) {
		      return x;
		    }
		  }
		  return -1;
		}
	
	public int[] Numero () {

		
	int l= 10;
	int numeroBus = 5;
		
	int[] miArreglo = new int[l];
	
	for (int x=0; x<=l; x++) {
		miArreglo [9]= 5;	
		//System.out.println(miArreglo[x]);
	}	
	
	  int eleBus = existeArre(miArreglo, numeroBus);
	  if (eleBus == -1) {
	    System.out.println("El elemento NO existe en el arreglo");
	  } else {
	    System.out.println("El elemento existe en la posici칩n: " + eleBus);
	  }
	
	
	
	
	return null;
	}
	
	
public static String Nombres () {	
	
	
	int 침=20;
	String[] arregloNombres = new String [침];
		
	
	
	for (int n=0; n<=침; n++) {
		arregloNombres [0]="Francisco";	
		
		//System.out.println(arregloNombres[n]);
		
	}
	
	return null;
}
	


}
